{
    "name": "project-template",
    "version": "1.0.0",
    "description": "Full setup with webpack, sass and maybe typescript",
    "main": "index.js",
    "scripts": {
        "watch": "webpack serve --config webpack.config.js --mode development",
        "build": "webpack --config webpack.config.js --mode development",
        "build:prod": "webpack --config webpack.config.js --mode production",
        "format": "prettier --write src/**/*.ts{,x}",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "clean": "rm -rf node_modules/ && rm -rf package-lock.json",
        "install:clean": "npm run clean && npm install"
    },
    "keywords": [
        "template"
    ],
    "author": "Joey",
    "license": "MIT",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "css-loader": "^5.2.7",
        "eslint": "^8.2.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "file-loader": "^6.2.0",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^4.5.2",
        "mini-css-extract-plugin": "^1.6.2",
        "node-sass": "^5.0.0",
        "prettier": "^2.5.1",
        "sass-loader": "^10.2.1",
        "style-loader": "^2.0.0",
        "svg-inline-loader": "^0.8.2",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^3.11.3"
    },
    "dependencies": {
        "ts-loader": "^8.3.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.5.5"
    }
}
